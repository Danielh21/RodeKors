@model IEnumerable<Røde_Kors.Models.Rekvirent>

@{
    ViewBag.Title = "ListOfRekvirant";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ListOfRekvirant</h2>

<p>
    @Html.ActionLink("Create New", "CreateRekvirant")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RekvirantNavn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Kontaktperson)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RekvirantNavn)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Kontaktperson)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.revkirentId }) |
            @Html.ActionLink("Details", "Details", new { id=item.revkirentId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.revkirentId })
        </td>
    </tr>
}

</table>
